{
  "name": "search-query-builder",
  "version": "1.0.0",
  "description": "A powerful and flexible search query interface inspired by Sentry's search functionality",
  "private": true,
  "type": "module",
  "keywords": [
    "react",
    "search",
    "query",
    "builder",
    "autocomplete",
    "dropdown",
    "filter",
    "sentry",
    "typescript",
    "ui",
    "component"
  ],
  "author": "Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/search-query-builder.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/search-query-builder/issues"
  },
  "homepage": "https://your-username.github.io/search-query-builder/",
  "scripts": {
    "dev": "vite",
    "dev:demo": "vite --config vite.demo.config.ts",
    "build": "vite build",
    "build:demo": "vite build --config vite.demo.config.ts",
    "preview": "vite preview",
    "preview:demo": "vite preview --config vite.demo.config.ts",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:final": "jest FinalTest.test.tsx --ci",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "docs:dev": "npm run dev:demo",
    "docs:build": "npm run build:demo",
    "docs:serve": "cd docs && npx serve .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@react-aria/button": "^3.13.1",
    "@react-aria/combobox": "^3.12.3",
    "@react-aria/focus": "^3.20.3",
    "@react-aria/gridlist": "^3.13.0",
    "@react-aria/i18n": "^3.12.9",
    "@react-aria/interactions": "^3.25.1",
    "@react-aria/listbox": "^3.14.4",
    "@react-aria/live-announcer": "^3.4.2",
    "@react-aria/menu": "^3.18.3",
    "@react-aria/overlays": "^3.27.1",
    "@react-aria/selection": "^3.24.1",
    "@react-aria/textfield": "^3.17.3",
    "@react-aria/utils": "^3.29.0",
    "@react-aria/visually-hidden": "^3.8.23",
    "@react-stately/collections": "^3.12.4",
    "@react-stately/combobox": "^3.10.5",
    "@react-stately/list": "^3.12.2",
    "@react-stately/overlays": "^3.6.16",
    "@react-types/shared": "^3.29.1",
    "classnames": "^2.3.1",
    "fuse.js": "^6.6.2",
    "react": "^19.1.0",
    "react-aria-components": "^1.10.1",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.0",
    "tailwindcss": "^3.4.16",
    "ts-jest": "^29.2.5",
    "typescript": "^5.8.3",
    "vite": "^6.0.7"
  }
}
